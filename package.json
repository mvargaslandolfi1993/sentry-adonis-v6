{
  "name": "sentry-adonis-v6",
  "version": "1.0.0",
  "description": "Sentry provider for adonisjs v6 framework",
  "main": "build/index.js",
  "engines": {
    "node": ">=21.0.0"
  },
  "type": "module",
  "files": [
    "build"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types.js",
    "./commands": "./build/commands/main.js",
    "./services/main": "./build/services/main.js",
    "./sentry_provider": "./build/providers/sentry_provider.js"
  },
  "license": "MIT",
  "homepage": "https://github.com/mvargaslandolfi1993/sentry-adonis-v6#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvargaslandolfi1993/sentry-adonis-v6.git"
  },
  "bugs": {
    "url": "https://github.com/mvargaslandolfi1993/sentry-adonis-v6/issues"
  },
  "keywords": [
    "adonis",
    "sentry"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "build": "npm run copyfiles && tsc"
  },
  "author": "Miguel Vargas",
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@sentry/node": "^7.114.0"
  }
}
